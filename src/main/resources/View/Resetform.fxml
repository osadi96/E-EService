<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="310.0" prefWidth="452.0" style="-fx-border-color: #c54766;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ResetformController">
   <children>
      <Label alignment="CENTER" layoutX="21.0" layoutY="28.0" prefHeight="17.0" prefWidth="368.0" text="Enter OTP">
         <font>
            <Font name="System Bold" size="34.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="-2.0" layoutY="97.0" prefHeight="17.0" prefWidth="414.0" text="Check your Email for the OTP" textFill="#e46008">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtOTP" alignment="CENTER" layoutX="173.0" layoutY="190.0" prefHeight="25.0" prefWidth="107.0" promptText="One Time Password" />
      <Button layoutX="373.0" layoutY="252.0" mnemonicParsing="false" onAction="#resetButtonOnAction" text="Reset">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
      <JFXTextField fx:id="txtEmail" alignment="CENTER" layoutX="127.0" layoutY="143.0" prefHeight="25.0" prefWidth="190.0" promptText="Enter Email" />
      <Button layoutX="280.0" layoutY="252.0" mnemonicParsing="false" onAction="#sendotpButtonOnAction" text="Send">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
